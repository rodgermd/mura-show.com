<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>{% block title %}MuraShow.com{% endblock %}</title>
    {% block stylesheets %}
    <link rel="stylesheet" type="text/css" href="http://yui.yahooapis.com/2.9.0/build/reset/reset-min.css">
    <link rel="stylesheet" type="text/css" href="/css/default.css">
    <link rel="stylesheet" type="text/css" href="/css/ui.css">
    {% endblock %}
    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
  </head>
  <body>
    {% block header %}
      <div id="header">
        <a href="{{ path('homepage') }}" title="{{ 'go to start page' | trans }}" id="sitelogo">murashow.com</a>
        {% block subsite_name %}{% endblock subsite_name %}
        {% block primary_menu %}{% endblock primary_menu %}
        {% block secondary_menu %}{% endblock secondary_menu %}
        <div id="user-data">
          {% include "RodgerUserBundle:Default:user.login.html.twig" %}
        </div>
      </div>  
    {% endblock header %}
    <div id="content-wrapper">
    {% block body %}{% endblock %}
    </div>
    {% block javascripts %}
      <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
      <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1/jquery-ui.min.js"></script>
    {% endblock %}
    {% include "::google_analytics.html.twig" %}
  </body>
</html>
